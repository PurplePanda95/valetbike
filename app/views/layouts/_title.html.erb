<div class="grid-container">
  <div class = "logo">
  </div>
    <h1> <%= link_to "ValetBike", pages_home_path %></h1>
    <% if user_signed_in? && current_user.is_admin?%>
    <h1> <%= link_to "Admin Stations View", stations_path %></h1>
    <% elsif user_signed_in? && !current_user.ongoing_rental?%>
    <h1> <%= link_to "Rent a bike", stations_path %></h1>
    <% elsif user_signed_in? && current_user.ongoing_rental?%>
    <h1> <%= link_to "Return bike", stations_path %></h1>
    <h1> <%= link_to "Current Rental", rental_path(current_user.current_rental) %></h1>
    <% else %>
    <h1> <%= link_to "View Stations", stations_path %></h1>
    <% end %>
    <% if user_signed_in? %>
    <h1> <%= button_to "Log out", destroy_user_session_path, method: :delete %> </h1> 
    <% end %>
  </div>